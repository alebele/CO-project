      <Button layoutX="863.0" layoutY="603.0" mnemonicParsing="false" onAction="#switchToMainScene" prefHeight="40.0" prefWidth="150.0" style="-fx-background-color: #6B8E23;" text="Go back!" textFill="#dae8ba">
