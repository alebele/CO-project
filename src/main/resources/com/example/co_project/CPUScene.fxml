<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.co_project.CPUScene">
   <children>
      <ImageView fx:id="ImageViewCPU" fitHeight="720.0" fitWidth="1080.0" pickOnBounds="true" preserveRatio="true">
